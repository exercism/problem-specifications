{
  "exercise": "clock",
  "comments": [
    "Most languages require constructing a clock with initial values,",
    "adding or subtracting some number of minutes, and testing equality",
    "in some language-native way.  Negative and out of range values are",
    "generally expected to wrap around rather than represent errors."
  ],
  "cases": [
    {
      "description": "Create a new clock with an initial time",
      "cases": [
        {
          "uuid": "a577bacc-106b-496e-9792-b3083ea8705e",
          "description": "on the hour",
          "property": "create",
          "input": {
            "hour": 8,
            "minute": 0
          },
          "expected": "08:00"
        },
        {
          "uuid": "b5d0c360-3b88-489b-8e84-68a1c7a4fa23",
          "description": "past the hour",
          "property": "create",
          "input": {
            "hour": 11,
            "minute": 9
          },
          "expected": "11:09"
        },
        {
          "uuid": "473223f4-65f3-46ff-a9f7-7663c7e59440",
          "description": "midnight is zero hours",
          "property": "create",
          "input": {
            "hour": 24,
            "minute": 0
          },
          "expected": "00:00"
        },
        {
          "uuid": "ca95d24a-5924-447d-9a96-b91c8334725c",
          "description": "hour rolls over",
          "property": "create",
          "input": {
            "hour": 25,
            "minute": 0
          },
          "expected": "01:00"
        },
        {
          "uuid": "f3826de0-0925-4d69-8ac8-89aea7e52b78",
          "description": "hour rolls over continuously",
          "property": "create",
          "input": {
            "hour": 100,
            "minute": 0
          },
          "expected": "04:00"
        },
        {
          "uuid": "a02f7edf-dfd4-4b11-b21a-86de3cc6a95c",
          "description": "sixty minutes is next hour",
          "property": "create",
          "input": {
            "hour": 1,
            "minute": 60
          },
          "expected": "02:00"
        },
        {
          "uuid": "8f520df6-b816-444d-b90f-8a477789beb5",
          "description": "minutes roll over",
          "property": "create",
          "input": {
            "hour": 0,
            "minute": 160
          },
          "expected": "02:40"
        },
        {
          "uuid": "c75c091b-47ac-4655-8d40-643767fc4eed",
          "description": "minutes roll over continuously",
          "property": "create",
          "input": {
            "hour": 0,
            "minute": 1723
          },
          "expected": "04:43"
        },
        {
          "uuid": "06343ecb-cf39-419d-a3f5-dcbae0cc4c57",
          "description": "hour and minutes roll over",
          "property": "create",
          "input": {
            "hour": 25,
            "minute": 160
          },
          "expected": "03:40"
        },
        {
          "uuid": "be60810e-f5d9-4b58-9351-a9d1e90e660c",
          "description": "hour and minutes roll over continuously",
          "property": "create",
          "input": {
            "hour": 201,
            "minute": 3001
          },
          "expected": "11:01"
        },
        {
          "uuid": "1689107b-0b5c-4bea-aad3-65ec9859368a",
          "description": "hour and minutes roll over to exactly midnight",
          "property": "create",
          "input": {
            "hour": 72,
            "minute": 8640
          },
          "expected": "00:00"
        },
        {
          "uuid": "d3088ee8-91b7-4446-9e9d-5e2ad6219d91",
          "description": "negative hour",
          "property": "create",
          "input": {
            "hour": -1,
            "minute": 15
          },
          "expected": "23:15"
        },
        {
          "uuid": "77ef6921-f120-4d29-bade-80d54aa43b54",
          "description": "negative hour rolls over",
          "property": "create",
          "input": {
            "hour": -25,
            "minute": 0
          },
          "expected": "23:00"
        },
        {
          "uuid": "359294b5-972f-4546-bb9a-a85559065234",
          "description": "negative hour rolls over continuously",
          "property": "create",
          "input": {
            "hour": -91,
            "minute": 0
          },
          "expected": "05:00"
        },
        {
          "uuid": "509db8b7-ac19-47cc-bd3a-a9d2f30b03c0",
          "description": "negative minutes",
          "property": "create",
          "input": {
            "hour": 1,
            "minute": -40
          },
          "expected": "00:20"
        },
        {
          "uuid": "5d6bb225-130f-4084-84fd-9e0df8996f2a",
          "description": "negative minutes roll over",
          "property": "create",
          "input": {
            "hour": 1,
            "minute": -160
          },
          "expected": "22:20"
        },
        {
          "uuid": "d483ceef-b520-4f0c-b94a-8d2d58cf0484",
          "description": "negative minutes roll over continuously",
          "property": "create",
          "input": {
            "hour": 1,
            "minute": -4820
          },
          "expected": "16:40"
        },
        {
          "uuid": "1cd19447-19c6-44bf-9d04-9f8305ccb9ea",
          "description": "negative sixty minutes is previous hour",
          "property": "create",
          "input": {
            "hour": 2,
            "minute": -60
          },
          "expected": "01:00"
        },
        {
          "uuid": "9d3053aa-4f47-4afc-bd45-d67a72cef4dc",
          "description": "negative hour and minutes both roll over",
          "property": "create",
          "input": {
            "hour": -25,
            "minute": -160
          },
          "expected": "20:20"
        },
        {
          "uuid": "51d41fcf-491e-4ca0-9cae-2aa4f0163ad4",
          "description": "negative hour and minutes both roll over continuously",
          "property": "create",
          "input": {
            "hour": -121,
            "minute": -5810
          },
          "expected": "22:10"
        }
      ]
    },
    {
      "description": "Add minutes",
      "cases": [
        {
          "uuid": "d098e723-ad29-4ef9-997a-2693c4c9d89a",
          "description": "add minutes",
          "property": "add",
          "input": {
            "hour": 10,
            "minute": 0,
            "value": 3
          },
          "expected": "10:03"
        },
        {
          "uuid": "b6ec8f38-e53e-4b22-92a7-60dab1f485f4",
          "description": "add no minutes",
          "property": "add",
          "input": {
            "hour": 6,
            "minute": 41,
            "value": 0
          },
          "expected": "06:41"
        },
        {
          "uuid": "efd349dd-0785-453e-9ff8-d7452a8e7269",
          "description": "add to next hour",
          "property": "add",
          "input": {
            "hour": 0,
            "minute": 45,
            "value": 40
          },
          "expected": "01:25"
        },
        {
          "uuid": "749890f7-aba9-4702-acce-87becf4ef9fe",
          "description": "add more than one hour",
          "property": "add",
          "input": {
            "hour": 10,
            "minute": 0,
            "value": 61
          },
          "expected": "11:01"
        },
        {
          "uuid": "da63e4c1-1584-46e3-8d18-c9dc802c1713",
          "description": "add more than two hours with carry",
          "property": "add",
          "input": {
            "hour": 0,
            "minute": 45,
            "value": 160
          },
          "expected": "03:25"
        },
        {
          "uuid": "be167a32-3d33-4cec-a8bc-accd47ddbb71",
          "description": "add across midnight",
          "property": "add",
          "input": {
            "hour": 23,
            "minute": 59,
            "value": 2
          },
          "expected": "00:01"
        },
        {
          "uuid": "6672541e-cdae-46e4-8be7-a820cc3be2a8",
          "description": "add more than one day (1500 min = 25 hrs)",
          "property": "add",
          "input": {
            "hour": 5,
            "minute": 32,
            "value": 1500
          },
          "expected": "06:32"
        },
        {
          "uuid": "1918050d-c79b-4cb7-b707-b607e2745c7e",
          "description": "add more than two days",
          "property": "add",
          "input": {
            "hour": 1,
            "minute": 1,
            "value": 3500
          },
          "expected": "11:21"
        }
      ]
    },
    {
      "description": "Subtract minutes",
      "cases": [
        {
          "uuid": "37336cac-5ede-43a5-9026-d426cbe40354",
          "description": "subtract minutes",
          "property": "subtract",
          "input": {
            "hour": 10,
            "minute": 3,
            "value": 3
          },
          "expected": "10:00"
        },
        {
          "uuid": "0aafa4d0-3b5f-4b12-b3af-e3a9e09c047b",
          "description": "subtract to previous hour",
          "property": "subtract",
          "input": {
            "hour": 10,
            "minute": 3,
            "value": 30
          },
          "expected": "09:33"
        },
        {
          "uuid": "9b4e809c-612f-4b15-aae0-1df0acb801b9",
          "description": "subtract more than an hour",
          "property": "subtract",
          "input": {
            "hour": 10,
            "minute": 3,
            "value": 70
          },
          "expected": "08:53"
        },
        {
          "uuid": "8b04bb6a-3d33-4e6c-8de9-f5de6d2c70d6",
          "description": "subtract across midnight",
          "property": "subtract",
          "input": {
            "hour": 0,
            "minute": 3,
            "value": 4
          },
          "expected": "23:59"
        },
        {
          "uuid": "07c3bbf7-ce4d-4658-86e8-4a77b7a5ccd9",
          "description": "subtract more than two hours",
          "property": "subtract",
          "input": {
            "hour": 0,
            "minute": 0,
            "value": 160
          },
          "expected": "21:20"
        },
        {
          "uuid": "90ac8a1b-761c-4342-9c9c-cdc3ed5db097",
          "description": "subtract more than two hours with borrow",
          "property": "subtract",
          "input": {
            "hour": 6,
            "minute": 15,
            "value": 160
          },
          "expected": "03:35"
        },
        {
          "uuid": "2149f985-7136-44ad-9b29-ec023a97a2b7",
          "description": "subtract more than one day (1500 min = 25 hrs)",
          "property": "subtract",
          "input": {
            "hour": 5,
            "minute": 32,
            "value": 1500
          },
          "expected": "04:32"
        },
        {
          "uuid": "ba11dbf0-ac27-4acb-ada9-3b853ec08c97",
          "description": "subtract more than two days",
          "property": "subtract",
          "input": {
            "hour": 2,
            "minute": 20,
            "value": 3000
          },
          "expected": "00:20"
        }
      ]
    },
    {
      "description": "Compare two clocks for equality",
      "cases": [
        {
          "uuid": "f2fdad51-499f-4c9b-a791-b28c9282e311",
          "description": "clocks with same time",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 15,
              "minute": 37
            },
            "clock2": {
              "hour": 15,
              "minute": 37
            }
          },
          "expected": true
        },
        {
          "uuid": "5d409d4b-f862-4960-901e-ec430160b768",
          "description": "clocks a minute apart",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 15,
              "minute": 36
            },
            "clock2": {
              "hour": 15,
              "minute": 37
            }
          },
          "expected": false
        },
        {
          "uuid": "a6045fcf-2b52-4a47-8bb2-ef10a064cba5",
          "description": "clocks an hour apart",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 14,
              "minute": 37
            },
            "clock2": {
              "hour": 15,
              "minute": 37
            }
          },
          "expected": false
        },
        {
          "uuid": "66b12758-0be5-448b-a13c-6a44bce83527",
          "description": "clocks with hour overflow",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 10,
              "minute": 37
            },
            "clock2": {
              "hour": 34,
              "minute": 37
            }
          },
          "expected": true
        },
        {
          "uuid": "2b19960c-212e-4a71-9aac-c581592f8111",
          "description": "clocks with hour overflow by several days",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 3,
              "minute": 11
            },
            "clock2": {
              "hour": 99,
              "minute": 11
            }
          },
          "expected": true
        },
        {
          "uuid": "6f8c6541-afac-4a92-b0c2-b10d4e50269f",
          "description": "clocks with negative hour",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 22,
              "minute": 40
            },
            "clock2": {
              "hour": -2,
              "minute": 40
            }
          },
          "expected": true
        },
        {
          "uuid": "bb9d5a68-e324-4bf5-a75e-0e9b1f97a90d",
          "description": "clocks with negative hour that wraps",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 17,
              "minute": 3
            },
            "clock2": {
              "hour": -31,
              "minute": 3
            }
          },
          "expected": true
        },
        {
          "uuid": "56c0326d-565b-4d19-a26f-63b3205778b7",
          "description": "clocks with negative hour that wraps multiple times",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 13,
              "minute": 49
            },
            "clock2": {
              "hour": -83,
              "minute": 49
            }
          },
          "expected": true
        },
        {
          "uuid": "c90b9de8-ddff-4ffe-9858-da44a40fdbc2",
          "description": "clocks with minute overflow",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 0,
              "minute": 1
            },
            "clock2": {
              "hour": 0,
              "minute": 1441
            }
          },
          "expected": true
        },
        {
          "uuid": "533a3dc5-59a7-491b-b728-a7a34fe325de",
          "description": "clocks with minute overflow by several days",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 2,
              "minute": 2
            },
            "clock2": {
              "hour": 2,
              "minute": 4322
            }
          },
          "expected": true
        },
        {
          "uuid": "fff49e15-f7b7-4692-a204-0f6052d62636",
          "description": "clocks with negative minute",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 2,
              "minute": 40
            },
            "clock2": {
              "hour": 3,
              "minute": -20
            }
          },
          "expected": true
        },
        {
          "uuid": "605c65bb-21bd-43eb-8f04-878edf508366",
          "description": "clocks with negative minute that wraps",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 4,
              "minute": 10
            },
            "clock2": {
              "hour": 5,
              "minute": -1490
            }
          },
          "expected": true
        },
        {
          "uuid": "b87e64ed-212a-4335-91fd-56da8421d077",
          "description": "clocks with negative minute that wraps multiple times",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 6,
              "minute": 15
            },
            "clock2": {
              "hour": 6,
              "minute": -4305
            }
          },
          "expected": true
        },
        {
          "uuid": "822fbf26-1f3b-4b13-b9bf-c914816b53dd",
          "description": "clocks with negative hours and minutes",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 7,
              "minute": 32
            },
            "clock2": {
              "hour": -12,
              "minute": -268
            }
          },
          "expected": true
        },
        {
          "uuid": "e787bccd-cf58-4a1d-841c-ff80eaaccfaa",
          "description": "clocks with negative hours and minutes that wrap",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 18,
              "minute": 7
            },
            "clock2": {
              "hour": -54,
              "minute": -11513
            }
          },
          "expected": true
        },
        {
          "uuid": "96969ca8-875a-48a1-86ae-257a528c44f5",
          "description": "full clock and zeroed clock",
          "property": "equal",
          "input": {
            "clock1": {
              "hour": 24,
              "minute": 0
            },
            "clock2": {
              "hour": 0,
              "minute": 0
            }
          },
          "expected": true
        }
      ]
    }
  ]
}
